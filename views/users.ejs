<%- include('header', {title: title}) %>

<table class="ui compact celled definition inverted table">
  <thead class="full-width">
    <tr>
      <th>Tools</th>
      <th>Username</th>
      <th>Flag</th>
      <th>Status</th>
      <th>Registered</th>
      <th>Expiry</th>
    </tr>
  </thead>
  <tbody>
    <% for(var i=0; i < users.length; i++) { %>
     <tr>
       <td class="collapsing">
         <div class="ui fitted slider checkbox">
           <button class="ui icon black button">
            <i class="edit icon"></i>
          </button>
         </div>
       </td>
       <td><%= users[i].name %></td>
       <td><%= users[i].status.code %></td>
       <td><%= users[i].status.message %></td>
       <td></td>
       <td></td>
     </tr>
    <% } %>
  </tbody>
  <tfoot class="full-width">
    <tr>
      <th></th>
      <th colspan="5">
        <div class="ui right floated small labeled icon black button">
          <i class="user icon"></i> Add User
        </div>
      </th>
    </tr>
  </tfoot>
</table>

<%- include('footer') %>
